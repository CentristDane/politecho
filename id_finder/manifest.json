{
  "name": "Get pages source",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "scoring.js",
      "jquery-3.1.1.min.js",
      "parse.js"
    ],
    "persistent": true
  },
  "permissions": ["tabs", "<all_urls>", "storage"]
}
